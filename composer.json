{
    "name": "ibanawx/prooph-event-store-rest-api-bundle",
    "description": "Make Prooph Event Store streams accessible over a REST API.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "David Twomey",
            "email": "ibanawx@gmail.com"
        }
    ],
    "require": {
        "symfony/framework-bundle": "^3.1",
        "prooph/common": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "Ibanawx\\Bundle\\Prooph\\EventStore\\RestApiBundle\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6",
        "fzaninotto/faker": "^1.6"
    },
    "autoload-dev": {
        "psr-4": {
            "Ibanawx\\Bundle\\Prooph\\EventStore\\RestApiBundle\\Tests\\": "tests/",
            "Ibanawx\\Bundle\\Prooph\\EventStore\\RestApiBundle\\Tests\\Unit\\": "tests/unit/"
        }
    }
}
